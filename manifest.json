{
	"manifest_version":2,
	"name": "Save As MHTML",
	"description": "Save As MHTML",
	"version": "2.0",
	"content_scripts": [{
		"all_frames": false,
		"run_at": "document_start",
		"js": [ "js/contentscript.js" ],
		"matches": [
			"<all_urls>"
		]
	}],
	"background": {
		"scripts": ["js/background.js","js/jszip/jszip.min.js"],
		"persistent": false
	},
	"icons": {
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
	"permissions": [ 
		"tabs",
		"pageCapture",
		"webNavigation",
		"downloads",
		"contextMenus"
	]
}
